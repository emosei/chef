{
  "name": "web",
  "chef_type": "role",
  "json_class": "Chef::Role",
  "default_attributes": {
    "ruby-env": {
      "version": "2.4.0",
      "rbenv_url":      "https://github.com/rbenv/rbenv",
      "ruby-build_url": "https://github.com/rbenv/ruby-build"
    },
    "apache": {
      "listen": ["*:80", "*:443"]
    }
  },
  "run_list":[
    "recipe[yum-epel]",
    "recipe[ruby-env]",
    "recipe[nodejs]",
    "recipe[apache2]",
    "recipe[apache2::mod_ssl]",
    "recipe[palm-httpd::passenger]",
    "recipe[imagemagick]"
  ]
}
